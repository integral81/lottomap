	// 마이 홈페이지 메인 조회
	fn_selectMyHomeInfo : function(){
		const param = {
		}
		const options = {
			"method": "GET",
			"async" : true
		}
		ajaxUtil.sendHttpJson(param, navL.path + "/selectMyHomeInfo.do", options, function(code, msg, {data}){
